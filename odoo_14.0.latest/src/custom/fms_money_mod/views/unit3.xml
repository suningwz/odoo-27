<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <record id="gastos_view" model="ir.ui.view">
            <field name="name">expenses.view</field>
            <field name="model">fsm.order</field>
            <field name="inherit_id" ref="fieldservice.fsm_order_form" />
            <field name="priority" eval="31"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@name='execution_page']" name="bag" position="inside">
                    <group string="Dineros utilizados" name="dinners_bag" attrs="{'invisible': [('ejecucion_tecnica', '!=', 'r1')]}">
                        <field name="gastos_tecnico" widget="many2many" nolabel="1">
                            <tree editable="bottom">
                                <field name="date" />
                                <field name="name" />
                                <field name="employee_id" />
                                <field name="total_amount"/>
                                <field name="state"/>
                                <field name="company_id"/>
                            </tree>
                        </field>
                    </group>
                </xpath>
            </field>
       </record>
    </data>
</odoo>