<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <record id="codigo_unidades" model="ir.ui.view">
            <field name="name">stock.view</field>
            <field name="model">fsm.order</field>
            <field name="inherit_id" ref="fieldservice.fsm_order_form" />
            <field name="priority" eval="31"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@name='instructions_page']" name="elements" position="before">
                    <page string="Elementos del tecnico">
                        <field name="inventario" widget="many2many"/>
                    </page>
                </xpath>
            </field>
       </record>
    </data>
</odoo>
